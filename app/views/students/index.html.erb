<h1><%= t('activemodel.action.title.listing', :model => Student.model_name.human) %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.register_number %></td>
        <td><%= student.name %></td>
        <td><%= student.status %></td>
        <td><%= link_to t('activemodel.show'), student %></td>
        <td><%= link_to t('activemodel.edit'), edit_student_path(student) %></td>
        <td><%= link_to t('activemodel.destroy'), student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_student_path %>
